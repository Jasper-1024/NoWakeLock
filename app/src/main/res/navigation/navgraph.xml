<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navgraph"
    app:startDestination="@id/appListFragment">

    <fragment
        android:id="@+id/appListFragment"
        android:name="com.js.nowakelock.ui.appList.AppListFragment"
        android:label="AppListFragment">
        <action
            android:id="@+id/action_appListFragment_to_wakeLockFragment"
            app:destination="@id/wakeLockFragment" />
    </fragment>

    <fragment
        android:id="@+id/wakeLockFragment"
        android:name="com.js.nowakelock.ui.wakeLock.WakeLockFragment"
        android:label="fragment_wake_lock"
        tools:layout="@layout/fragment_wakelock">
        <argument
            android:name="PackageName"
            android:defaultValue='""'
            app:argType="string" />
    </fragment>
</navigation>